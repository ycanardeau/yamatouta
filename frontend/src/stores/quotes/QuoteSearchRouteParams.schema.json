{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"properties": {
		"page": {
			"type": "number"
		},
		"pageSize": {
			"type": "number"
		},
		"query": {
			"type": "string"
		},
		"quoteType": {
			"enum": ["Haiku", "Lyrics", "Other", "Tanka", "Word"],
			"type": "string"
		},
		"sort": {
			"enum": ["CreatedAsc", "CreatedDesc", "UpdatedAsc", "UpdatedDesc"],
			"type": "string"
		}
	},
	"type": "object"
}
