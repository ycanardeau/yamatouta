{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"properties": {
		"page": {
			"type": "number"
		},
		"pageSize": {
			"type": "number"
		},
		"query": {
			"type": "string"
		},
		"sort": {
			"enum": [
				"CreatedAsc",
				"CreatedDesc",
				"NameAsc",
				"NameDesc",
				"UpdatedAsc",
				"UpdatedDesc"
			],
			"type": "string"
		},
		"workType": {
			"enum": ["Book", "Other", "Song", "Unspecified"],
			"type": "string"
		}
	},
	"type": "object"
}
