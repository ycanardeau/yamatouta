{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"properties": {
		"page": {
			"type": "number"
		},
		"pageSize": {
			"type": "number"
		},
		"query": {
			"type": "string"
		},
		"sort": {
			"enum": ["CreatedAsc", "CreatedDesc", "NameAsc", "NameDesc"],
			"type": "string"
		},
		"userGroup": {
			"enum": [
				"Admin",
				"AdvancedUser",
				"LimitedUser",
				"Mod",
				"SeniorMod",
				"User"
			],
			"type": "string"
		}
	},
	"type": "object"
}
